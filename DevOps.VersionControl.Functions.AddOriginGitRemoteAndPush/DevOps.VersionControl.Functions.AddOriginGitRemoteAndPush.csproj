<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>cdorst</Authors>
    <Description>Helper for adding a git remote and pushing a branch</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>CDorst.DevOps.VersionControl.Functions.AddOriginGitRemoteAndPush</PackageId>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CDorst.DevOps.VersionControl.Functions.RunGitPushCommand" Version="1.0.0" />
    <PackageReference Include="CDorst.DevOps.VersionControl.Functions.RunGitRemoteCommand" Version="1.0.0" />
  </ItemGroup>

</Project>
